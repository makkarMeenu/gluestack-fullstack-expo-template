{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/components/*": [
        "./components/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@/config/*": [
        "./config/*"
      ],
      "@/generated/hooks/*": [
        "./graphql/hooks/*"
      ],
      "@/apollo/*": [
        "./graphql/apollo/*"
      ],
      "@/context/*": [
        "./context/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/services/*": [
        "./services/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}